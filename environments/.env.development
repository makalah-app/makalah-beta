# DEVELOPMENT ENVIRONMENT CONFIGURATION
# This file contains safe defaults for development
# Real secrets should be in .env.local (never committed)

NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Development logging
LOG_LEVEL=debug
ENABLE_DEBUG_MODE=true
ENABLE_TESTING_MODE=true
ENABLE_DEBUG_PANEL=true

# Development database (local Supabase)
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24ifQ.example
SUPABASE_JWT_SECRET=super-secret-jwt-token-with-at-least-32-characters-long

# NextAuth configuration
